{
  "name": "template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "pnpm env:debug&&nest start",
    "start:dev": "pnpm env:debug&&nest start --watch",
    "start:debug": "pnpm env:debug&&nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm env:debug&&jest",
    "test:watch": "pnpm env:debug&&jest --watch",
    "test:cov": "pnpm env:debug&&jest --coverage",
    "test:debug": "pnpm env:debug&&node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm env:debug&&jest --config ./test/jest-e2e.json",
    "docker": "pnpm env:prod&&docker-compose up --build -d --remove-orphans",
    "docker:dev": "pnpm env:dev&&docker-compose -f docker-compose.yml -f docker-compose.infra.yml -f docker-compose.debug.yml up --build -d --remove-orphans",
    "docker:debug": "pnpm env:debug&&docker-compose -f docker-compose.infra.yml -f docker-compose.debug.yml up -d --remove-orphans",
    "docker:down": "pnpm env:prod&&docker-compose -f docker-compose.yml -f docker-compose.infra.yml -f docker-compose.debug.yml down",
    "env:prod": "bash ./script/load-config.bash prod",
    "env:dev": "bash ./script/load-config.bash dev",
    "env:debug": "bash ./script/load-config.bash debug",
    "prisma:g": "sh ./script/prisma.sh generate",
    "prisma:i": "sh ./script/prisma.sh push",
    "prisma:m:user": "sh ./script/prisma.sh migrate user ",
    "prisma:m:operationrecord": "sh ./script/prisma.sh migrate operation_record "
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^5.11.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cron": "^3.1.6",
    "htmlparser": "^1.7.7",
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sprintf-js": "^1.1.3",
    "svg-captcha": "^1.4.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/sprintf-js": "^1.1.4",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}